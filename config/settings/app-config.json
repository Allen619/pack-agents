{
  "app": {
    "name": "Pack Agents",
    "version": "1.0.0",
    "environment": "development"
  },
  "storage": {
    "type": "file",
    "configRoot": "./config",
    "autoBackup": true,
    "maxExecutionHistory": 1000
  },
  "execution": {
    "defaultTimeout": 300000,
    "maxRetries": 3,
    "parallelLimit": 5
  }
}