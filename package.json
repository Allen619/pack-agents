{
  "name": "pack-agents",
  "version": "1.0.0",
  "description": "多Agent工作流管理平台 - 基于Claude Code SDK的AI工具平台",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build",
    "init-config": "node scripts/init-config.js",
    "docs:dev": "vitepress dev docs-site",
    "docs:build": "vitepress build docs-site",
    "docs:preview": "vitepress preview docs-site"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.0",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/pro-components": "^2.6.0",
    "@ant-design/x": "^1.0.0",
    "@anthropic-ai/claude-code": "latest",
    "@anthropic-ai/sdk": "^0.63.0",
    "@langchain/core": "^0.1.0",
    "@langchain/langgraph": "^0.0.10",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.8.0",
    "@xyflow/react": "^12.8.5",
    "ajv": "^8.12.0",
    "antd": "^5.12.0",
    "clsx": "^2.1.1",
    "immer": "^10.0.3",
    "nanoid": "^5.0.3",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.3.6",
    "winston": "^3.11.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@playwright/test": "^1.55.0",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.0.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitepress": "^1.6.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
